<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<!-- $CwuRes: src/regi/www/webapps/rnreg/web/WEB-INF/web.xml,v 1.1 2003/02/19 22:38:54 hartd Exp $ -->

<web-app>


    <display-name>RnReg</display-name>
    <description>
      This application permits a user to register a device for use in
      the dynamic network environment.
    </description>

    <servlet>
      <servlet-name>ResNetReg</servlet-name>
      <servlet-class>ResNetReg</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>ResNetReg</servlet-name>
      <url-pattern>/ResNetReg</url-pattern>
    </servlet-mapping>

    <session-config>
      <session-timeout>15</session-timeout>
    </session-config>

</web-app>
